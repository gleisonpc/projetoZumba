<Window x:Class="projetoZumba.Alunos"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Alunos" Height="484" Width="699" Loaded="Window_Loaded" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="AlunosNovo" Content="Novo Aluno" Margin="10,24,538,399" Click="AlunosNovo_Click"></Button>
        <DataGrid x:Name="DataGridAlunos" ItemsSource="{Binding}" CanUserAddRows="False" CanUserDeleteRows="False" Margin="0,80,0,0" MouseDoubleClick="DataGridAlunos_MouseDoubleClick"></DataGrid>
    </Grid>
</Window>
